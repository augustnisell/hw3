<h1><%= @place["name"] %></h1>

<a href="/places">Back to Places</a>

<% for post in @posts %>
    <p>
        <h2><%= post["title"] %></h2>
        <%= post["posted_on"] %>
        <br>
        <%= post["description"] %>
    </p>
<% end %>

<p>
    <a href="/posts/new?place_id=<%= @place["id"] %>">New Post</a>
</p>